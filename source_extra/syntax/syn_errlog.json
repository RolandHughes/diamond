{
   "classes": [
      "\\bQ[A-Za-z0-9]+\\b(?!/)",
      "\\bCS[A-Za-z0-9]+\\b"
   ],
      "comment-multi-start": "(?!E)E",
      "comment-multi-end": "(?!E)E",
      "comment-single": "(?!E)E",
   "functions": [
      "^.*:\\d+:\\d+: note",
      "operator==",
      "operand of"
   ],
   "ignore-case" : false,
   "keywords": [
      "^make.+:",
      "^.*:\\d+:\\d+: error",
      "\\bclass\\b",
      "\\bconst\\b",
      "\\btypename\\b",
      "\\busing\\b"
   ],
   "types": [
   ]
}
